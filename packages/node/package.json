{
  "name": "@signalwire/node",
  "version": "0.0.0",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "esmodule": "./dist/index.modern.mjs",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.modern.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "microbundle watch -f es,modern --define process.env.NODE_ENV=development --tsconfig ./tsconfig.build.json --target node",
    "build": "pkg-typings --pre-run && microbundle --external none -f modern,es,cjs --define process.env.NODE_ENV=production --tsconfig ./tsconfig.build.json --target node && pkg-typings --after-run",
    "test": "NODE_ENV=test jest",
    "prepare": "npm run build",
    "docs": "typedoc --options typedoc.js",
    "docs:watch": "typedoc --options typedoc.js --watch"
  },
  "dependencies": {
    "@signalwire/core": "0.0.0",
    "node-abort-controller": "^2.0.0",
    "node-fetch": "^2.6.1",
    "swagger-typescript-api": "^9.1.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.10",
    "msw": "^0.28.2"
  },
  "types": "dist/node/src/index.d.ts"
}
